{
  "name": "up-crawler",
  "version": "1.0.0",
  "description": "Robust puppeteer-based sign-up automation for Upwork",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc && node scripts/fix-imports.js",
    "start": "node dist/main.js",
    "start:upwork": "node dist/main.js upwork",
    "start:turnstile": "node dist/main.js turnstile --out out/turnstile_results.json",
    "dev": "tsx src/main.ts",
    "migrate": "tsx src/migrate.ts",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "clean": "rm -rf dist",
    "db:up": "docker compose up -d postgres",
    "db:down": "docker compose down",
    "db:reset": "docker compose down -v && docker compose up -d postgres",
    "db:logs": "docker compose logs postgres",
    "setup": "npm run db:up && sleep 5 && npm run migrate"
  },
  "dependencies": {
    "@types/pdfkit": "^0.17.2",
    "cmd-ts": "^0.11.0",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.3.1",
    "jsdom": "^24.0.0",
    "kysely": "^0.25.0",
    "pdfkit": "^0.17.1",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "puppeteer": "^22.1.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "puppeteer-extra-plugin-user-data-dir": "^2.4.1",
    "puppeteer-extra-plugin-user-preferences": "^2.4.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
